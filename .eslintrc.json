{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": ["eslint-config-prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error"
  },
  "plugins": ["eslint-plugin-prettier"]
}
